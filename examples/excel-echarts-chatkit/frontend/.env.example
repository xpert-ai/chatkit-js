# Copy to `.env` and fill in values.

# Optional - Backend target for Vite proxy (defaults to http://localhost:8010)
VITE_BACKEND_TARGET=http://localhost:8010

# Optional - Direct backend URL (leave empty to use proxy)
VITE_BACKEND_ORIGIN=

# Required - The assistant ID configured with render_echarts client tool
VITE_CHATKIT_ASSISTANT_ID=your-assistant-id

# Optional - ChatKit iframe URL (defaults to built-in)
VITE_CHATKIT_TARGET=http://localhost:5176
