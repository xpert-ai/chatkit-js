# Copy to `.env` and fill in values.

# Required
XPERTAI_API_KEY=

# Optional (defaults to https://api.mtda.cloud/api/ai)
# Example (if you proxy OpenAI through a local gateway): http://localhost:3000/api/ai
CHATKIT_API_BASE=

# Optional (can also be provided by the frontend request body)
CHATKIT_ASSISTANT_ID=

# Optional (comma-separated)
CORS_ALLOW_ORIGINS=http://localhost:5173

# Optional (used by /api/chat)
CHAT_MODEL=gpt-4o-mini
